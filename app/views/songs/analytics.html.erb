<div style="background: white; color: black;">
	<b>song listens</b><br>
	<% @slistens.reverse.each do |listen| %>
		<% song = Song.find(listen.song_id) %>
		<%= distance_of_time_in_words(listen.created_at - Time.now)%> &nbsp;&nbsp;&nbsp; id ~ <%= listen.id %>  &nbsp;&nbsp;&nbsp; <%= song.song_artist%> - <%= song.song_name %> &nbsp;&nbsp;&nbsp; user ~
			<% if listen.user_id == 0 %>
				0
			<% else %>
				<%= User.find(listen.user_id).username %>
			<% end %>
		<br>
	<% end %>
	<br><br>
	<b>like</b><br>
	<% song = Song.find(@likez.song_id) %>
	<%= distance_of_time_in_words(@likez.created_at - Time.now) %> song ~ <%= song.song_artist  %> - <%= song.song_name %> &nbsp;&nbsp;&nbsp; user_id ~ <%= @likez.user.username %>
	<br><br>
	<b>songs</b><br>
	<%= distance_of_time_in_words(@songz.created_at - Time.now) %> &nbsp;&nbsp;&nbsp; <%= @songz.song_artist %> - <%= @songz.song_name %>&nbsp;&nbsp;&nbsp; user ~ <%= @songz.author.username %>
	<br><br>
	<b>remark</b><br>
	<%= distance_of_time_in_words(@remarkz.created_at - Time.now)%> 
	<% if @remarkz.user_id == 0 %>
		0 ~ <%= @remarkz.body %>
	<% else %>
		<%= @remarkz.user.username %> ~ <%= @remarkz.body %>
	<% end %>
	<br><br>
	<b>user</b><br>
	<%= distance_of_time_in_words(@userz.created_at - Time.now) %> &nbsp;&nbsp;&nbsp; id ~ <%= @userz.id.to_s %> &nbsp;&nbsp;&nbsp; <%= @userz.username %>
</div>