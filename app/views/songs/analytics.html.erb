<b>song listens</b><br>
<% @slistens.reverse.each do |listen| %>
	<%= distance_of_time_in_words(listen.created_at - Time.now)%> &nbsp;&nbsp;&nbsp; id ~ <%= listen.id %>  &nbsp;&nbsp;&nbsp; song_id ~ <%= listen.song_id %> &nbsp;&nbsp;&nbsp; user_id ~
		<% if listen.user_id == 0 %>
			0
		<% else %>
			<%= User.find(listen.user_id).username %>
		<% end %>
	<br>
<% end %>
<br><br>
<b>like</b><br>
<%= distance_of_time_in_words(@likez.created_at - Time.now) %> song_id ~ <%= @likez.song_id %> &nbsp;&nbsp;&nbsp; user_id ~ <%= @likez.user.username %>
<br><br>
<b>songs</b><br>
<%= distance_of_time_in_words(@songz.created_at - Time.now) %> &nbsp;&nbsp;&nbsp; <%= @songz.song_artist %> - <%= @songz.song_name %>&nbsp;&nbsp;&nbsp; user ~ <%= @songz.author.username %>
<br><br>
<b>remark</b><br>
<%= distance_of_time_in_words(@remarkz.created_at - Time.now)%> 
<% if @remarkz.user_id == 0 %>
	0 ~ <%= @remarkz.body %>
<% else %>
	<%= @remarkz.user.username %> ~ <%= @remarkz.body %>
<% end %>