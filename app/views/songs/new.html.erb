<h3>.submit</h3>
<%= form_for(@song) do |f| %>
  <%= f.label :song_artist %>
  <br>
  <%= f.text_field :song_artist %>
  <br>
  <%= f.label :song_name %>
  <br>
  <%= f.text_field :song_name %>
  <br>
  <%= f.label :song_link %>
  <br>
  <%= f.text_field :song_link %>
  <br>
  <%= f.submit %>
<% end %>