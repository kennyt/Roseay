<div class="form">
  <h3>submit</h3>
  <%= form_for(@song) do |f| %>
    <%= f.label :song_artist %>

    <%= f.text_field :song_artist %>
    <br>
    <%= f.label :song_name %>

    <%= f.text_field :song_name %>
    <br>
    <%= f.label :song_link %>

    <%= f.text_field :song_link %>
    <br>
    <br>
    <%= f.submit 'submit' %>
  <% end %>
</div>