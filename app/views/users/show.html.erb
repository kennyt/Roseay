<%= link_to 'home', root_path %>
<br>
<h1><%= @user.username %></h1>
<ol>
  <h2>submissions</h2>
  <% @submissions.each do |link| %>
    <%= link_to "#{link.song_artist} - #{link.song_name}", link.song_link %>
    <%= "| #{link.points} points" %>
  <% end %>
  <h2>upvoted*</h2><style type="text/css">a {text-decoration: none}</style>
  <% @liked_songs.each do |song| %>
    <%= link_to "#{song.song_artist} - #{song.song_name}", song.song_link %>
    <%= "| #{song.points} points" %>
  <% end %>
</ol>